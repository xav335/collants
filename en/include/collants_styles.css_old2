/****************** collants, bas, promotions, coup de coeur, top5 *******************/

/*Style for the background-bar*/
.clBar{position:absolute; width:10; height:10; background-color:""; layer-background-color:""; visibility:hidden}

/*Styles for level 0*/
.clLevel0,.clLevel0over{position:absolute; padding:1px; font-family: arial,helvetica,tahoma; font-size:12px; ; font-weight:bold}
.clLevel0{color:#000000; font-family: FrutigerNextLT arial,helvetica,tahoma;;}
.clLevel0over{background-color:""; color:#B60734; cursor:pointer; cursor:hand; }

/****************** par marque - par catégorie *******************/

/*Style for the background-bar*/
.cl2Bar{position:absolute; width:10; height:10; background-color:""; layer-background-color:""; visibility:hidden}

/*Styles for level 0*/
.cl2Level0,.cl2Level0over{position:absolute; padding:2px; font-family:tahoma,arial,helvetica; font-size:12px; font-weight:bold}
.cl2Level0{background-color:#bb274b; layer-background-color:""; color:#000000;}
.cl2Level0over{background-color:#ffffff; layer-background-color:#ffffff; color:#B60734; cursor:pointer; cursor:hand; }
.cl2Level0border{position:absolute; visibility:hidden; background-color:#ffffff; layer-background-color:#ffffff}

/*Styles for level 1*/
.cl2Level1, .cl2Level1over{position:absolute; padding:2px; font-family:tahoma,arial,helvetica; font-size:11px; font-weight:bold}
.cl2Level1{background-color:#ffffff; layer-background-color:#ffffff; color:#000000;}
.cl2Level1over{background-color:#e1e1e1; layer-background-color:#ffffff; color:#000000; cursor:pointer; cursor:hand; }
.cl2Level1border{position:absolute; visibility:hidden; background-color:#ffffff; layer-background-color:#ffffff}
