/****************** valeur generales par defaut **********/
body{
	text-align : left;
	font-family : Verdana;
	font-size : 11px;
	font-style: normal;
	font-weight : normal;
	color : #000000;
    overflow:auto; 
	scrollbar-face-color : #c21845; 
	scrollbar-shadow-color : #B60734; 
	scrollbar-highlight-color : #B60734; 
	scrollbar-3dlight-color : #B60734; 
	scrollbar-darkshadow-color : #B60734; 
	scrollbar-track-color : #c02e53; 
	scrollbar-arrow-color : #000000;
}
/*********************************/
#crochet{
	color: #ffffff; 
	font-size: 20px ;
	font-weight : bold; 
}
#crochet_menu{
	color: #B60734; 
	font-size: 12px;
	font-weight : bold; 
}
#crochet_menu2{
	color: #ffffff; 
	font-size: 12px;
	font-weight : bold; 
}	
/************** Liens	**********************/
#accueil:link{
	text-decoration:none ;
	font-family : verdana, arial, sans-serif;
	font-size : 16px;
	font-weight : bold; 
	color:#ffffff ;
}
#accueil:visited {
	text-decoration:none ;
	font-family : verdana, arial, sans-serif;
	font-size : 16px;
	font-weight : bold; 
	color:#ffffff ;
}
#accueil:hover {
	text-decoration:none ; 
	font-family : verdana, arial, sans-serif;
	font-size : 16px;
	font-weight : bold; 
	color:#000000 ;
}
#menu:link{
	text-decoration:none ;
	font-family : verdana, arial, sans-serif;
	font-size : 11px;
	font-weight : bold; 
	color:#000000 ;
}
#menu:visited {
	text-decoration:none ;
	font-family : verdana, arial, sans-serif;
	font-size : 11px;
	font-weight : bold; 
	color:#000000 ;
}
#menu:hover {
	text-decoration:none ; 
	font-family : verdana, arial, sans-serif;
	font-size : 11px;
	font-weight : bold; 
	color:#B60734 ;
}
#contact{
	text-align : left;
	font-family : Verdana;
	font-size : 12px;
	font-style: normal;
	font-weight : normal;
	color : #000000;
	text-decoration: underline;
}

a.contact:hover{
	text-align : left;
	font-family : Verdana;
	font-size : 12px;
	font-style: normal;
	font-weight : normal;
	color : #000000;
	text-decoration: none;
}
